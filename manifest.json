{
  "manifest_version": 2,

  "name": "Opendirectories Search",
  "description": "This extension uses Google to search for open directories",
  "version": "2.3",

  "browser_action": {
    "default_icon": "app/assets/images/app.png",
    "default_popup": "app/views/app.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}